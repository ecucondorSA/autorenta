{
  "name": "autorenta",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "e2e": "nx run-many --target=e2e --all",
    "start": "nx serve",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:e2e": "nx affected --target=e2e",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "show-migrations": "nx migrate show",
    "generate-migration": "nx migrate generate",
    "run-migration": "nx migrate run",
    "docker:build": "docker build -t autorenta .",
    "docker:run": "docker run -p 3000:3000 autorenta",
    "prepare": "husky install",
    "nx": "nx",
    "deploy": "serverless deploy",
    "remove": "serverless remove"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "18.0.7",
    "@nx/jest": "18.0.7",
    "@nx/js": "18.0.7",
    "@nx/linter": "18.0.7",
    "@nx/nest": "18.0.7",
    "@nx/node": "18.0.7",
    "@nx/prettier": "18.0.7",
    "@nx/webpack": "18.0.7",
    "@types/jest": "29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "27.2.1",
    "husky": ">=7",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "lint-staged": ">=10",
    "nx": "18.0.7",
    "prettier": "^2.6.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
