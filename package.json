    "release": "turbo build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.1.1",
    "turbo": "latest"
  },
  "dependencies": {
    "@nx/eslint-plugin-nx": "16.0.0-beta.1"
  }
}
