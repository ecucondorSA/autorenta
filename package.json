  "autorenta": {
    "version": "0.1.0",
    "architecture": "angular-standalone",
    "backend": "supabase",
    "deployment": "cloudflare-pages",
    "patterns": {
      "services": "inject-pattern",
      "components": "standalone-lazy",
      "testing": "jasmine-karma",
      "routing": "lazy-loaded-routes",
      "stateManagement": "signals",
      "storage": "supabase-storage-rls"
    },
    "conventions": {
      "fileNaming": "kebab-case",
      "componentSuffix": ".component.ts",
      "serviceSuffix": ".service.ts",
      "pageSuffix": ".page.ts",
      "modelSuffix": ".model.ts"
    },
    "features": [
      "auth",
      "cars",
      "bookings",
      "payments",
      "admin"
    ],
    "documentation": {
      "architecture": "CLAUDE.md",
      "patterns": "PATTERNS.md",
      "skills": "CLAUDE_SKILLS_GUIDE.md",
      "workflows": "tools/claude-workflows.sh"
    },
    "claude": {
      "skills": {
        "enabled": true,
        "recommended": [
          "angular-scaffolder",
          "supabase-rls-debugger",
          "typescript-sync",
          "test-generator",
          "performance-optimizer",
          "security-auditor"
        ]
      },
      "autoBackground": {
        "enabled": true,
        "timeout": 900000
      }
    }
  },
  "dependencies": {
    "@supabase/storage-js": "^2.86.0",
    "chart.js": "^4.5.1",
    "csv-parse": "",
