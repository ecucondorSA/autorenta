  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npx ng build --max_old_space_size=4096",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --watch=false --browsers=ChromeHeadlessCI",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "compodoc:serve": "npx compodoc -s tsconfig.doc.json",
    "prebuild": "node scripts/env/patch-stencil-empty-glob.js && node scripts/env/generate-env.js",
    "postinstall": "node patch-modules.js",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start",
    "storybook": "ng storybook",
    "build-storybook": "ng build-storybook",
    "chromatic": "npx chromatic",
    "prepare": "husky install",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "npm run build && firebase deploy --only functions",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "functions:shell": "firebase functions:shell",
    "functions:log": "firebase functions:log",
    "functions:delete": "firebase functions:delete",
    "functions:config:get": "firebase functions:config:get",
    "functions:config:set": "firebase functions:config:set",
    "functions:config:unset": "firebase functions:config:unset",
    "functions:secrets:set": "firebase functions:secrets:set",
    "functions:secrets:get": "firebase functions:secrets:get",
    "functions:secrets:delete": "firebase functions:secrets:delete",
    "api": "nest start",
    "api:dev": "nest start --watch",
    "api:debug": "nest start --debug --watch",
    "api:prod": "node dist/main",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:create": "npm run typeorm migration:create -- -n",
    "seed": "ts-node ./seeds/run-seed.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
