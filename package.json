    "test:e2e:calendar:headed": "playwright test tests/e2e/google-calendar-oauth.spec.ts --headed",
    "test:e2e:calendar:debug": "playwright test tests/e2e/google-calendar-oauth.spec.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:coverage:report": "cd apps/web && bash scripts/generate-coverage-report.sh",
