    "primeicons": "^7.0.0",
    "primeng": "^20.4.0",
    "shepherd.js": "^14.5.1",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@compodoc/compodoc",
      "@parcel/watcher",
      "@scarf/scarf",
      "@sentry/capacitor",
      "@sentry/cli",
      "core-js",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
