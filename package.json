    "depurar:chrome": "./scripts/chrome-dev.sh",
    "depurar:cdp": "./scripts/debug-cdp.sh",
    "tests:cdp": "./scripts/test-with-cdp.sh",
    "tests:cdp:ui": "./scripts/test-with-cdp.sh --ui",
    "debug:mercadopago": "node tools/scripts/debug-mercadopago-sdk.mjs",
    "debug:mp": "node tools/scripts/debug-mercadopago-sdk.mjs",
    "_comment_build": "=== BUILDING ===",
    "build": "./tools/run.sh build",
    "build:web": "./tools/run.sh build:web",
    "build:worker": "./tools/run.sh build:worker",
    "_comment_deploy": "=== DEPLOYMENT ===",
    "deploy": "./tools/run.sh deploy",
    "deploy:web": "./tools/run.sh deploy:web",
    "deploy:worker": "./tools/run.sh deploy:worker",
    "deploy:worker:ai-car-generator": "./tools/run.sh deploy:worker:ai-car-generator",
    "_comment_ci": "=== CI/CD ===",
    "ci": "./tools/run.sh ci",
    "lint": "./tools/run.sh lint",
    "guardrails": "tsx scripts/maintenance/guardrails.ts",
    "guardrails:strict": "npx tsx scripts/maintenance/guardrails.ts --strict",
    "guardrails:baseline": "npx tsx scripts/maintenance/guardrails.ts --update-baseline",
    "lint:fix": "./tools/run.sh lint:fix",
    "lint:orphans": "npx tsx scripts/lint-orphans.ts --fix-suggestions",
    "lint:orphans:strict": "npx tsx scripts/lint-orphans.ts --strict --fix-suggestions",
    "lint:orphans:json": "npx tsx scripts/lint-orphans.ts --json > orphan-lint-report.json",
    "format": "./tools/run.sh format",
    "_comment_utils": "=== UTILITIES ===",
    "install": "AUTORENTA_SKIP_INSTALL=1 ./tools/run.sh install",
    "install:all": "./tools/run.sh install",
    "prepare": "husky",
    "clean": "./tools/ops/cleanup.sh",
    "clean:deep": "./tools/ops/deep-clean.sh",
    "sync:types": "./tools/dev/sync-types.sh",
    "sync:types:remote": "./tools/run.sh sync:types:remote",
    "status": "./tools/run.sh status",
    "_comment_setup": "=== SETUP (one-time) ===",
    "setup:auth": "./tools/setup/setup-auth.sh",
    "setup:prod": "./tools/setup/setup-production.sh",
    "_comment_monitor": "=== MONITORING ===",
    "monitor:health": "./tools/ops/monitor-health.sh",
    "monitor:wallet": "./tools/run.sh monitor:wallet",
    "simulate:fgo": "npx tsx apps/web/tools/coverage-simulator.ts",
    "check:auth": "./tools/archive/check-auth.sh",
    "_comment_browser": "=== BROWSER AUTOMATION ===",
    "bridge": "cd browser-extension && npm run bridge",
    "bridge:dev": "cd browser-extension && npm run bridge:dev",
    "bridge:test": "node browser-extension/test-integration.mjs",
    "browser:extension:install": "echo 'Open chrome://extensions and load unpacked: /home/edu/autorenta/browser-extension'",
    "browser:setup": "cd browser-extension && npm install && npm run bridge",
    "_comment_accessibility": "=== ACCESSIBILITY & TYPOGRAPHY ===",
    "check:contrast": "node tools/audit/check-color-contrast.mjs",
    "check:font-sizes": "node tools/audit/check-font-sizes.mjs",
    "check:overflow": "npx tsx tools/audit/detect-overflow-issues.ts",
    "check:a11y": "npm run check:contrast && npm run check:font-sizes",
    "font:subset": "node tools/dev/font-subset.mjs",
    "_comment_legacy": "=== LEGACY (kept for compatibility) ===",
    "workflows": "./tools/archive/claude-workflows.sh",
    "check:skills": "tools/audit/check-skills.sh",
    "accounting:export": "node tools/accounting/export-ledger.mjs",
    "types:db:gen": "supabase gen types typescript --project-id pisqjmoklivzpwufhscx > supabase.types.generated.ts",
    "types:db:check": "npm run types:db:gen && git diff --exit-code supabase.types.generated.ts",
    "validate:colors": "node tools/audit/validate-colors.mjs",
    "monitoring:setup": "./tools/setup/monitoring-setup.sh",
    "monitoring:health": "./tools/ops/monitor-health.sh",
    "monitoring:deploy": "supabase functions deploy monitoring-health-check && supabase functions deploy monitoring-alerts && supabase functions deploy monitoring-metrics"
  },
