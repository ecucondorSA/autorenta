{
  "name": "autorenta",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "e2e": "nx run-many --target=e2e --all",
    "start": "nx serve",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:e2e": "nx affected --target=e2e",
    "affected:dep-graph": "nx dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "show-migrations": "nx migrate show",
    "migrate": "nx migrate",
    "clear-cache": "nx reset",
    "prepare": "husky install",
    "ci": "nx run-many --target=lint --all && nx run-many --target=test --all && nx run-many --target=build --all",
    "nx": "nx",
    "deploy": "nx run api:deploy"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@nx/devkit": "18.0.7",
    "@nx/eslint-plugin": "18.0.7",
    "@nx/jest": "18.0.7",
    "@nx/js": "18.0.7",
    "@nx/nest": "18.0.7",
    "@nx/node": "18.0.7",
    "@nx/prettier": "18.0.7",
    "@nx/webpack": "18.0.7",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": ">=7",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "18.0.7",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  }
}