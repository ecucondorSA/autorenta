{
  "mcpServers": {
    "cloudflare-builds": {
      "type": "http",
      "url": "https://builds.mcp.cloudflare.com/mcp"
    },
    "cloudflare-docs": {
      "type": "http",
      "url": "https://docs.mcp.cloudflare.com/mcp"
    },
    "cloudflare-bindings": {
      "type": "http",
      "url": "https://bindings.mcp.cloudflare.com/mcp"
    },
    "mercadopago-mcp-server-prod": {
      "type": "http",
      "url": "https://mcp.mercadopago.com/mcp",
      "headers": {
        "Authorization": "Bearer ${MERCADOPAGO_ACCESS_TOKEN}"
      }
    },
    "autorenta-platform": {
      "command": "node",
      "args": [
        "/home/edu/autorenta/mcp-server/dist/index.js"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "browser-extension": {
      "command": "node",
      "args": [
        "/home/edu/autorenta/browser-extension/mcp-server.js"
      ]
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=pisqjmoklivzpwufhscx"
    }
  }
}