{
  "name": "autorenta-web",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0 <=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node tools/start-with-env.mjs",
    "prebuild": "node scripts/generate-env.js",
    "build": "ng build",
    "postbuild": "node scripts/create-cloudflare-config.js",
    "lint": "ng lint",
    "test": "ng test",
    "format": "prettier --cache --write \"src/**/*.{ts,html,css,scss}\"",
    "prepare": "husky install",
    "deploy:pages": "npm run build && CLOUDFLARE_ACCOUNT_ID=5b448192fe4b369642b68ad8f53a7603 npx wrangler pages deploy dist/web/browser --project-name=autorenta-web",
    "worker:dev": "cd ../../functions/workers/payments_webhook && npm run dev",
    "worker:deploy": "cd ../../functions/workers/payments_webhook && npm run deploy"
  },
  "lint-staged": {
    "src/**/*.{ts,js,html,css,scss}": [
      "prettier --cache --write",
      "ng lint --fix --silent"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~20.3.7",
    "@angular/common": "~20.3.7",
    "@angular/compiler": "~20.3.7",
    "@angular/core": "~20.3.7",
    "@angular/forms": "~20.3.7",
    "@angular/platform-browser": "~20.3.7",
    "@angular/router": "~20.3.7",
    "@angular/service-worker": "~20.3.7",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/mercadopago": "^1.5.11",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "mapbox-gl": "^3.15.0",
    "mercadopago": "^2.9.0",
    "onnxruntime-web": "^1.23.0",
    "rxjs": "~7.8.0",
    "shepherd.js": "^14.5.1",
    "supercluster": "^8.0.1",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.25.76",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.4.0",
    "@angular-eslint/eslint-plugin": "^20.4.0",
    "@angular-eslint/eslint-plugin-template": "^20.4.0",
    "@angular-eslint/template-parser": "^20.4.0",
    "@angular/build": "~20.3.7",
    "@angular/cli": "~20.3.7",
    "@angular/compiler-cli": "~20.3.7",
    "@playwright/test": "^1.56.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.4",
    "onnxruntime-node": "^1.23.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.2"
  }
}
