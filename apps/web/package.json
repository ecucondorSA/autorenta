{
  "name": "autorenta-web",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0 <=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node tools/start-with-env.mjs",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "format": "prettier --cache --write \"src/**/*.{ts,html,css,scss}\"",
    "prepare": "husky install",
    "deploy:pages": "npm run build && npx wrangler pages deploy dist/autorenta-web",
    "worker:dev": "cd ../../functions/workers/payments_webhook && npm run dev",
    "worker:deploy": "cd ../../functions/workers/payments_webhook && npm run deploy"
  },
  "lint-staged": {
    "src/**/*.{ts,js,html,css,scss}": [
      "prettier --cache --write",
      "ng lint --fix --silent"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.5",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.25.76",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.4.0",
    "@angular-eslint/eslint-plugin": "^20.4.0",
    "@angular-eslint/eslint-plugin-template": "^20.4.0",
    "@angular-eslint/template-parser": "^20.4.0",
    "@angular/build": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.9.2"
  }
}
