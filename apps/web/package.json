{
  "name": "autorenta-web",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0 <=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node tools/start-with-env.mjs > app_start.log 2>&1",
    "start:no-hmr": "ng serve --configuration development-no-hmr",
    "clean-restart": "./scripts/clean-restart.sh",
    "prebuild": "node scripts/generate-env.js",
    "build": "npx ng build",
    "postinstall": "node scripts/patch-stencil-empty-glob.js",
    "postbuild": "node scripts/create-cloudflare-config.js",
    "sentry:sourcemaps": "./scripts/upload-sourcemaps.sh",
    "lint": "node_modules/.bin/ng lint",
    "test": "ng test",
    "test:quick": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "format": "prettier --cache --write \"src/**/*.{ts,html,css,scss}\"",
    "deploy:pages": "npm run build && CLOUDFLARE_ACCOUNT_ID=5b448192fe4b369642b68ad8f53a7603 npx wrangler pages deploy dist/web/browser --project-name=autorentar",
    "worker:dev": "cd ../../functions/workers/payments_webhook && npm run dev",
    "worker:deploy": "cd ../../functions/workers/payments_webhook && npm run deploy",
    "validate": "npm run lint && npm run build",
    "analyze": "node_modules/.bin/ng build --stats-json && npx -y webpack-bundle-analyzer dist/web/stats.json -m static -r dist/web/report.html",
    "preview": "npm run build && npx -y wrangler pages dev dist/web/browser",
    "serve:ssr:web": "node dist/web/server/server.mjs",
    "docs:build": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -s",
    "docs:clean": "rm -rf documentation",
    "cap:sync": "npx cap sync android",
    "cap:open": "npx cap open android",
    "deploy:android": "./scripts/deploy-android.sh",
    "deploy:android:fast": "./scripts/deploy-android.sh --no-icons",
    "android:debug": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:release": "npm run build && npx cap sync android && cd android && ./gradlew bundleRelease"
  },
  "lint-staged": {
    "src/**/*.{ts,js,html,css,scss}": [
      "prettier --cache --write",
      "ng lint --fix --silent"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-server": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@angular/service-worker": "~20.3.15",
    "@angular/ssr": "^20.3.13",
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.1",
    "@capacitor/camera": "^7.0.3",
    "@capacitor/core": "^7.4.4",
    "@capacitor/geolocation": "^7.1.6",
    "@capacitor/haptics": "^7.0.3",
    "@capacitor/local-notifications": "^7.0.4",
    "@capacitor/motion": "^7.0.2",
    "@capacitor/preferences": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.4",
    "@capacitor/share": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.4",
    "@capacitor/status-bar": "^7.0.4",
    "@capgo/capacitor-native-biometric": "^7.6.0",
    "@ionic/angular": "^8.7.11",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@sentry/angular": "9.46.0",
    "@sentry/capacitor": "^2.4.1",
    "@supabase/storage-js": "^2.86.2",
    "@supabase/supabase-js": "^2.86.2",
    "@types/flatpickr": "^3.1.4",
    "@types/mapbox-gl": "^3.4.1",
    "@types/mercadopago": "^1.5.11",
    "date-fns": "^4.1.0",
    "flatpickr": "^4.6.13",
    "ionicons": "^8.0.13",
    "mapbox-gl": "^3.17.0",
    "mercadopago": "^2.11.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.4.0",
    "rxjs": "~7.8.2",
    "shepherd.js": "^14.5.1",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.25.76",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.13",
    "@angular-eslint/builder": "^20.7.0",
    "@angular-eslint/eslint-plugin": "^20.7.0",
    "@angular-eslint/eslint-plugin-template": "^20.7.0",
    "@angular-eslint/template-parser": "^20.7.0",
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "~20.3.15",
    "@angular/ssr": "^20.3.7",
    "@axe-core/playwright": "^4.11.0",
    "@capacitor/cli": "^7.4.4",
    "@cloudflare/workers-types": "^4.20251205.0",
    "@compodoc/compodoc": "^1.1.32",
    "@sentry/cli": "^2.58.2",
    "@types/express": "^5.0.6",
    "@types/jasmine": "~5.1.13",
    "@types/mercadopago-sdk-js": "^2.0.17",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "express": "^5.2.1",
    "glob": "^10.5.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.6"
  }
}
